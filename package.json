{
  "name": "prava-cash",
  "version": "1.0.0",
  "description": "Prava Cash - Cashflow Management Dashboard with Node.js backend",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "npm run dev --prefix client",
    "client:build": "cd client && npm install && npm run build",
    "client:preview": "npm run preview --prefix client",
    "build": "cd client && npm install && npm run build",
    "build:backend": "npm install"
  },
  "keywords": [
    "cashflow",
    "express",
    "sqlite",
    "dashboard"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "express-session": "^1.18.2",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.0",
    "pg": "^8.16.3",
    "socket.io": "^4.8.1",
    "sql.js": "^1.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
